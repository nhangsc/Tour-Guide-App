<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mytour">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:parentActivityName=".EatFragment"
            android:name=".EatFragment"
            android:label="@string/category_eat" />
        <activity
            android:parentActivityName=".VisitFragment"
            android:name=".VisitFragment"
            android:label="@string/category_visit" />
        <activity
            android:parentActivityName=".ShopFragment"
            android:name=".ShopFragment"
            android:label="@string/category_shop" />
        <activity
            android:parentActivityName=".StayFragment"
            android:name=".StayFragment"
            android:label="@string/category_stay" />
        <activity
            android:parentActivityName=".EatFragment"
            android:name=".DetailItem"
            android:label="@string/category_detail" />

    </application>

</manifest>